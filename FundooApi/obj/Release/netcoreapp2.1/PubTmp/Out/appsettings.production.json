{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    },
    "Seriallog": {
      "MinimumLevel": "Information",
      "WriteTo": [
        {
          "Name": "Literalconsole",
          "Args": {
            "outputTemplate": "{TimeStamp:HH:mm:ss}{Level}|{RequestId}-{Message}{NewLine}{Exception}"
          }
        }
      ]
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "UserDBConncetion": "Server=(localdb)\\MSSQLLocalDB;Database=FundooApi;Integrated Security=True"
  },
  "ApplicationSetting": {
    "JWT_Secret": "1234567890123456",
    "Issuer": "https://localhost:44349/",
    "Audience": "https://localhost:44349/"
  },
  "RedisSetting": {
    "host": "localhost",
    "port": 6379
  },
  "FundooAPI": {
    "commandName": "Project",
    "launchBrowser": true,
    "launchUrl": "http://localhost:5000/swagger",
    "environmentVariables": {
      "ASPNETCORE_ENVIRONMENT": "Development"
    }
  }
}